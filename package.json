{
  "name": "playwright-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "node -e \"require('fs').mkdirSync('downloads',{recursive:true})\"",
    "install:browsers": "npx --yes playwright install",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "codegen": "playwright codegen",
    "format": "npx --yes prettier -w ."
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "dotenv": "^16.6.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "description": "This project signs into a website and downloads data using Playwright tests.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "arg": "^4.1.3",
    "create-require": "^1.1.1",
    "diff": "^4.0.2",
    "make-error": "^1.3.6",
    "playwright": "^1.55.0",
    "playwright-core": "^1.55.0",
    "undici-types": "^7.10.0",
    "v8-compile-cache-lib": "^3.0.1",
    "yn": "^3.1.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
